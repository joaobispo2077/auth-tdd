{
  "name": "authTdd",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/joaobispo2077/auth-tdd.git",
  "author": "Jo√£o Bispo <joaobispo2077@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --transpile-only --ignore-watch node_modules src/server.ts",
    "test": "cross-env NODE_ENV=test jest --runInBand",
    "test:clearCache": "cross-env NODE_ENV=test jest --clearCache",
    "test:silent": "cross-env NODE_ENV=test jest --watchAll --silent --noStackTrace",
    "test:staged": "cross-env NODE_ENV=test jest --findRelatedTests",
    "prepare": "husky install",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "style:check": "prettier --check src/**/*.ts",
    "style:fix": "prettier --write src/**/*.ts",
    "commit:lint": "lint-staged",
    "typeorm": "ts-node-dev node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.32",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.1",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "sqlite3": "^5.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.2",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.3.2"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "prettier --write",
      "eslint --fix",
      "cross-env NODE_ENV=test jest --findRelatedTests",
      "git add -A"
    ]
  }
}
